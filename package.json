{
  "name": "vue-admin",
  "version": "3.6.0",
  "scripts": {
    "dev": "cross-env env_config=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "localTest": "cross-env env_config=localTest webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:prod": "cross-env  env_config=prod node build/build.js --host 0.0.0.0",
    "build:sit": "cross-env  env_config=sit node build/build.js",
    "dll": "webpack --config build/webpack.dll.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint"
  },
  "keywords": [
    "vue",
    "element-ui",
    "admin",
    "management-system"
  ],
